<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="pos_config_view_form_pos_sale_inherit" model="ir.ui.view">
        <field name="name">pos.config.form.pos.sale.inherit</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="pos_sale.pos_config_view_form_pos_sale"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='crm_team_id']" position="after">
                <div class="row mt16 o_settings_container" id="user_id">
                    <div class="col-xs-12 col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="user_id" class="col-md-3"/>
                            <field name="user_id"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

</odoo>